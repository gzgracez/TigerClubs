<% include ../header %>

<table class="table table-striped table-hover usersTable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Username</th>
      <th>Email</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i = 0; i < users.length; i++) {%>
      <tr>
          <td>
            <%= users[i]["firstName"] %> <%= users[i]["lastName"] %>
          </td>
          <td>
            <%= users[i]["username"] %>
          </td>
          <td>
            <a href="mailto:<%= users[i]["email"] %>" class='usersTable'><%= users[i]["email"] %></a>
          </td>
          <td>
            <%= users[i]["userType"] %>
          </td>
      </tr>
    <% } %>
  </tbody>
</table>

<% include ../footer %>