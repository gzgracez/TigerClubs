<% include ../header %>

<div class="container">
  <h1>All Clubs</h1>
  

  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Club Name</th>
        <th>Club Description</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i = 0; i < clubs.length; i++) {%>
        <tr>
            <td>
              <%= clubs[i]["clubname"] %>
            </td>
            <td>
              <%= clubs[i]["description"] %>
            </td>
            <td>
              <button type="button" class="btn btn-primary">Join</button>
            </td>
        </tr>
      <% } %>
    </tbody>
  </table>

</div>

<% include ../footer %>