<% include ../header %>
<p><strong>Description: </strong><%= club["description"] %></p>
<p><strong>Advisor: </strong><%= club["Advisor"] %></p>
<p><strong>Leaders: </strong></p>
	<% for(i = 0; i < club["leaders"].length; i++) {%>
		<p><%=club["leaders"][i]%></p>
	<%}%>
<table class="table table-striped table-hover">
<thead>
  <tr>
    <th>Announcement</th>
    <th>Description</th>
    <th>Author</th>
    <th>Date</th>
  </tr>
</thead>
<tbody>
  <% for(var i = 0; i < club["announcements"].length; i++) {%>
    <tr>
        <td>
          <%= club["announcements"]["name"] %>
        </td>
        <td>
          <%= club["announcements"]["description"] %>
        </td>
        <td>
          <%= club["announcements"]["authorID"] %>
        </td>
         <td>
          <%= club["announcements"]["postDate"] %>
        </td>

    </tr>
  <% } %>
</tbody>
</table>
<% include ../footer %>